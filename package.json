{
  "name": "starter-kit",
  "version": "1.0.1",
  "description": "ueno. starter kit",
  "engines": {
    "node": ">=6.0 <7",
    "npm": ">=3.8 <4",
    "yarn": ">=0.15 <1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server.js",
    "dev": "cross-env NODE_ENV=development node ./webpack/dev.js",
    "test": "karma start ./webpack/karma.js",
    "lint": "npm run eslint; npm run stylelint",
    "eslint": "eslint src",
    "stylelint": "stylelint \"src/**/*.scss\" --syntax scss",
    "build": "rm -rf ./build && cross-env NODE_ENV=production webpack --config ./webpack/release.js",
    "heroku-build": "if [ \"x$NODE_ENV\" != \"x\" ] && [ \"$NODE_ENV\" != \"development\" ]; then npm run build; fi;",
    "postinstall": "npm run heroku-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ueno-llc/starter-kit.git"
  },
  "keywords": [
    "react",
    "webpack",
    "starter-kit",
    "universal"
  ],
  "author": "ueno.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ueno-llc/starter-kit/issues"
  },
  "homepage": "https://github.com/ueno-llc/starter-kit#readme",
